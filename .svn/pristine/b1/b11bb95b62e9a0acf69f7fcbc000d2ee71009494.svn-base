    public class GroupActivityDetail
    {
        //活动头图
        public string ActivityHeadPicUrl { get; set; }
        /// <summary>
        /// 唯一标识
        /// </summary>
        public Nullable<int> SysNo { get; set; }

        /// <summary>
        /// 活动名称
        /// </summary>
        public string GroupActivityName { get; set; }

        /// <summary>
        /// 活动描述
        /// </summary>
        public string GroupActivityDes { get; set; }

        /// <summary>
        /// 当前团活动每个会员可参加和建团的总数量
        /// </summary>
